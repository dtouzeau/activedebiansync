{
  "sync_interval": 360,
  "sync_packages": true,
  "repository_path": "/var/lib/ActiveDebianSync/mirror",
  "debian_mirror": "http://deb.debian.org/debian",
  "sync_debian_installer": false,
  "sync_installer_udebs": false,
  "sync_installer_images": false,
  "cve_scanner_enabled": false,
  "cve_scan_after_sync": true,
  "cve_cache_expiry_hours": 6,
  "cve_nvd_enabled": false,
  "cve_nvd_api_key": "",
  "cve_osv_enabled": true,
  "installer_image_types": ["netboot", "hd-media", "cdrom"],
  "debian_releases": [
    "bookworm",
    "bookworm-updates",
    "bookworm-security",
    "trixie"
  ],
  "debian_architectures": [
    "amd64",
    "arm64"
  ],
  "debian_components": [
    "main",
    "contrib",
    "non-free",
    "non-free-firmware"
  ],
  "sync_translations": true,
  "translation_languages": ["en", "fr", "de", "es"],
  "http_enabled": true,
  "http_port": 8080,
  "https_enabled": true,
  "https_port": 8443,
  "web_console_enabled": false,
  "web_console_port": 8090,
  "web_console_listen_addr": "0.0.0.0",
  "web_console_base_path": "",
  "web_console_trusted_proxies": "127.0.0.1",
  "web_console_secure_cookies": true,
  "tls_cert_file": "/etc/ActiveDebianSync/server.crt",
  "tls_key_file": "/etc/ActiveDebianSync/server.key",
  "api_enabled": true,
  "api_port": 9090,
  "api_listen_addr": "0.0.0.0",
  "api_allowed_ips": [
    "192.168.0.0/16",
    "10.0.0.5"
  ],
  "max_disk_usage_percent": 85,
  "log_path": "/var/log/ActiveDebianSync/sync.log",
  "access_log_path": "/var/log/ActiveDebianSync/access.log",
  "sync_ubuntu_repository": false,
  "ubuntu_mirror": "http://archive.ubuntu.com/ubuntu",
  "ubuntu_releases": ["jammy", "noble"],
  "ubuntu_architectures": ["amd64"],
  "ubuntu_components": ["main", "restricted", "universe", "multiverse"],
  "ubuntu_release_configs": [
    {
      "name": "jammy",
      "is_lts": true,
      "sync_updates": true,
      "sync_backports": false,
      "sync_security": true,
      "sync_proposed": false
    },
    {
      "name": "noble",
      "is_lts": true,
      "sync_updates": true,
      "sync_backports": false,
      "sync_security": true,
      "sync_proposed": false
    }
  ]
}
